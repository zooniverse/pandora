  "scripts": {
    "_start": "NODE_ENV=staging BABEL_ENV=development webpack-dashboard -p 3001 -- webpack-dev-server --config ./webpack.config.js",
    "start": "export HEAD_COMMIT=${HEAD_COMMIT:-$(git rev-parse --short HEAD)}; npm run _start;",
    "test": "NODE_ENV=development BABEL_ENV=test mocha test/setup.js test/enzyme-configuration.js $(find test -name *.test.jsx) --compilers js:@babel/register || true",
    "test-travis": "NODE_ENV=development BABEL_ENV=test mocha test/setup.js test/enzyme-configuration.js $(find test -name *.test.jsx) --compilers js:@babel/register",
    "eslint": "eslint .",
    "_build": "BABEL_ENV=production NODE_ENV=staging webpack --config webpack.production.config.js",
    "build": "export HEAD_COMMIT=${HEAD_COMMIT:-$(git rev-parse --short HEAD)}; npm run _build;",
    "_deploy": "node ./bin/upload.js",
    "deploy": "export NODE_ENV=production; npm run build && npm run _deploy translations.zooniverse.org",
    "_build-production": "export NODE_ENV=production; npm run build",
    "stage": "export NODE_ENV=staging; npm run build && npm run _deploy pandora.zooniverse.org"
  }
}
